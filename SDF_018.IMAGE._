void mainImage( out vec4 fragColor, in vec2 fragCoord )
{ 
    IV2 f_c = IV2( I32(fragCoord.x - 0.5 ) , 
                   I32(fragCoord.y - 0.5 ) );;

    V_4 c4d = texelFetch( iChannel0 , f_c , 0 );


     fragColor = c4d;
}

